pub function sum (Array a, Int b, Int c) returns Int: 
 val Int s; 
 s = b + c; 
 if (s >= 2): 
  foreach (i of a): 
   s = s*i; 
  end 
 end
 else :
  s = 0; 
 end 
 return s; 
end

function ali () returns Nil: 
    val Array a;
    sum(a,2)
    al(a,2,3)
    sum(1,2,3)
end